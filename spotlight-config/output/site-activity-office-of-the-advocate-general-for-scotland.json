        "x": {
          "label": "Date",
          "key": ["_start_at", "_end_at"],
          "format": {
            "type": "dateRange",
            "format": "D MMM YYYY"
          }
        },
        "y": [
          {
            "label": "Desktop",
            "categoryId": "desktop",
            "format": "integer"
          },
          {
            "label": "Mobile",
            "categoryId": "mobile",
            "format": "integer"
          },
          {
            "label": "Tablet",
            "categoryId": "tablet",
            "format": "integer"
          }
        ]
